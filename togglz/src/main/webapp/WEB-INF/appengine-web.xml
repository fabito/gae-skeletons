<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>gae-skeletons</application>
	<version>togglz</version>
	<threadsafe>true</threadsafe>

	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
		<property name="org.togglz.DISABLE_JNDI_LOOKUPS" value="true" />
	</system-properties>
	
	<admin-console>
		<page name="Feature Toggles" url="/_ah/togglz/index" />
	</admin-console>
	
</appengine-web-app>